{
  "name": "@likec4/core",
  "version": "1.47.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "description": "A core package for LikeC4, containing types, api, utilities and guards.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "files": [
    "dist",
    "**/package.json",
    "!**/*.spec.ts",
    "!**/*.test-d.ts",
    "!**/__test__/*",
    "!**/__snapshots__/*",
    "!**/*.map"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    },
    "./builder": {
      "sources": "./src/builder/index.ts",
      "default": {
        "types": "./dist/builder/index.d.mts",
        "import": "./dist/builder/index.mjs",
        "default": "./dist/builder/index.mjs"
      }
    },
    "./geometry": {
      "sources": "./src/geometry/index.ts",
      "default": {
        "types": "./dist/geometry/index.d.mts",
        "import": "./dist/geometry/index.mjs",
        "default": "./dist/geometry/index.mjs"
      }
    },
    "./compute-view/relationships": {
      "sources": "./src/compute-view/relationships-view/index.ts",
      "default": {
        "types": "./dist/compute-view/relationships-view/index.d.mts",
        "import": "./dist/compute-view/relationships-view/index.mjs",
        "default": "./dist/compute-view/relationships-view/index.mjs"
      }
    },
    "./compute-view": {
      "sources": "./src/compute-view/index.ts",
      "default": {
        "types": "./dist/compute-view/index.d.mts",
        "import": "./dist/compute-view/index.mjs",
        "default": "./dist/compute-view/index.mjs"
      }
    },
    "./model": {
      "sources": "./src/model/index.ts",
      "default": {
        "types": "./dist/model/index.d.mts",
        "import": "./dist/model/index.mjs",
        "default": "./dist/model/index.mjs"
      }
    },
    "./styles": {
      "sources": "./src/styles/index.ts",
      "default": {
        "types": "./dist/styles/index.d.mts",
        "import": "./dist/styles/index.mjs",
        "default": "./dist/styles/index.mjs"
      }
    },
    "./types": {
      "sources": "./src/types/index.ts",
      "default": {
        "types": "./dist/types/index.d.mts",
        "import": "./dist/types/index.mjs",
        "default": "./dist/types/index.mjs"
      }
    },
    "./utils/graphology": {
      "sources": "./src/utils/graphology/index.ts",
      "default": {
        "types": "./dist/utils/graphology/index.d.mts",
        "import": "./dist/utils/graphology/index.mjs",
        "default": "./dist/utils/graphology/index.mjs"
      }
    },
    "./utils": {
      "sources": "./src/utils/index.ts",
      "default": {
        "types": "./dist/utils/index.d.mts",
        "import": "./dist/utils/index.mjs",
        "default": "./dist/utils/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@dagrejs/dagre": "1.1.8",
    "@mantine/colors-generator": "8.3.10",
    "@types/chroma-js": "^3.1.2",
    "@types/extend": "3.0.4",
    "@types/hast": "3.0.4",
    "@types/mdast": "4.0.4",
    "@types/natural-compare-lite": "^1.4.2",
    "@types/node": "~22.19.3",
    "@types/object-hash": "^3.0.6",
    "@types/unist": "3.0.3",
    "chroma-js": "^3.2.0",
    "defu": "^6.1.4",
    "fdir": "6.4.0",
    "graphology": "^0.26.0",
    "graphology-dag": "^0.4.1",
    "graphology-types": "^0.24.8",
    "immer": "^11.1.0",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-string": "^4.0.0",
    "mdast-util-to-hast": "^13.2.1",
    "mnemonist": "0.40.3",
    "natural-compare-lite": "^1.4.0",
    "object-hash": "^3.0.0",
    "obliterator": "^2.0.5",
    "oxlint": "1.35.0",
    "rehype-format": "5.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "10.0.1",
    "remark-gfm": "4.0.1",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "remeda": "^2.32.0",
    "turbo": "2.7.2",
    "typescript": "5.9.3",
    "unbuild": "3.5.0",
    "unified": "11.0.5",
    "vitest": "4.0.16",
    "@likec4/devops": "1.42.0",
    "@likec4/tsconfig": "1.46.1"
  },
  "scripts": {
    "typecheck": "tsc -b --verbose",
    "build": "unbuild",
    "lint": "run -T eslint src/ --fix",
    "lint:package": "pnpx publint ./package.tgz",
    "test": "vitest run --no-isolate",
    "test:watch": "vitest",
    "clean": "likec4ops clean",
    "pack": "pnpm pack"
  },
  "types": "dist/index.d.mts",
  "module": "dist/index.mjs"
}